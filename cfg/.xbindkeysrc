"sh -c 'pactl set-sink-mute 0 false ; pactl set-sink-volume 0 +5%'"
	XF86AudioRaiseVolume
"sh -c 'pactl set-sink-mute 0 false ; pactl set-sink-volume 0 -5%'"
	XF86AudioLowerVolume
"pactl set-sink-mute 0 toggle"
	XF86AudioMute
"light -A 5%"
	XF86MonBrightnessUp
"light -U 5%"
	XF86MonBrightnessDown
"cmus-remote --next"
	Mod4 + Next
"cmus-remote --prev"
	Mod4 + Prior
"cmus-remote --pause"
	Mod4 + Home
"cmus-remote --stop"
	Mod4 + End
"screenscrot"
	Print
"locknotify"
	Shift + Shift_L
