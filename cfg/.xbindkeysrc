"sh -c 'pactl set-sink-mute @DEFAULT_SINK@ false ; pactl set-sink-volume @DEFAULT_SINK@ +5%'"
	XF86AudioRaiseVolume
"sh -c 'pactl set-sink-mute @DEFAULT_SINK@ false ; pactl set-sink-volume @DEFAULT_SINK@ -5%'"
	XF86AudioLowerVolume
"pactl set-sink-mute @DEFAULT_SINK@ toggle"
	XF86AudioMute
"light -A 5%"
	XF86MonBrightnessUp
"light -U 5%"
	XF86MonBrightnessDown
"mpc next"
	Mod4 + Next
"mpc prev"
	Mod4 + Prior
"mpc toggle"
	Mod4 + Home
"mpc stop"
	Mod4 + End
"screenscrot"
	Print
"locknotify"
	Shift + Shift_L
